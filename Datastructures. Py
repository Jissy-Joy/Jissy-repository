print(List2) 
List2. append("mango") 
print(List2)
 
#accessing elements from a tuple
 
Tuple1=(5, 10,15,20) 
print(Tuple1) 
print(Tuple1[2])
print(Tuple1[1: ])
 
#deleting element from a dictionary 
 
Dict1={"albin":10,"salvin":20,"johnvin":30}
print(Dict1) 
del Dict1["albin"] 
